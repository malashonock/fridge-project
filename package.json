{
  "name": "fridge-project",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "lint": "nx run-many --targets=lint",
    "build:shared": "nx build fridge-shared-lib --watch",
    "build:shell": "nx build fridge-shell-app",
    "start:server": "cd json-server && nodemon server.mjs --ignore db.json --ignore 'public/*'",
    "start:shell": "nx serve fridge-shell-app",
    "start:shell:ru": "nx serve fridge-shell-app --configuration=development_ru --port=4201",
    "xi18n:shell": "cd ./apps/fridge-shell-app && nx extract-i18n fridge-shell-app --output-path=./apps/fridge-shell-app/src/locale && xliffmerge --profile xliffmerge.json ru"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/entity": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "leaflet": "^1.9.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-devkit/core": "~15.2.8",
    "@angular-devkit/schematics": "~15.2.8",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.8",
    "@angular/compiler-cli": "^15.2.0",
    "@angular/localize": "^15.2.9",
    "@nx/angular": "16.8.1",
    "@nx/eslint-plugin": "16.8.1",
    "@nx/js": "16.8.1",
    "@nx/linter": "16.8.1",
    "@nx/workspace": "16.8.1",
    "@rxjs-insights/devtools": "^0.5.0",
    "@rxjs-insights/plugin-webpack5": "^0.5.0",
    "@rxjs-insights/rxjs7": "^0.5.0",
    "@schematics/angular": "~15.2.8",
    "@types/jest": "^29.5.2",
    "@types/leaflet": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-marbles": "^3.0.3",
    "jest-preset-angular": "^13.1.1",
    "json-server": "^0.17.3",
    "multer": "^1.4.5-lts.1",
    "ng-packagr": "^15.2.2",
    "ngx-i18nsupport": "^0.17.1",
    "nodemon": "^3.0.1",
